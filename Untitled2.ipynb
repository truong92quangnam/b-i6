    "import turtle\n",
    "\n",
    "t=turtle.Turtle()\n",
    "\n",
    "t.shape(\"turtle\")\n",
    "t.pensize(2)\n",
    "t.forward(100)\n",
    "\n",
    "t.left(90)\n",
    "\n",
    "t.penup()\n",
    "t.forward(100)\n",
    "t.pendown()\n",
    "t.forward(100)\n",
    "t.left(120)\n",
    "t.forward(100)\n",
    "t.left(120) \n",
    "t.forward(100)\n",
    "t.goto(100,50)\n",
    "turtle.done()"
